{
  "routes": [
    {
      "source": "^/myapp/(.*)$",
      "target": "$1",
      "destination": "myapp",
      "csrfProtection": false,
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/xsjs/(.*)$",
      "target": "$1",
      "destination": "xsjs",
      "csrfProtection": false,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(.*)(.xsodata)",
      "destination": "xsjs",
      "authenticationType": "xsuaa"
    }
  ]
}
